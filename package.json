{
  "name": "nodetest",
  "version": "1.0.0",
  "description": "Locmote - Node Test",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src open:back lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:back": "babel-node tools/backServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "build": "babel-node tools/build.js && npm run open:dist",
    "open:dist": "babel-node tools/distServer.js",
    "clean-dist": "npm run rimraf ./dist && mkdir dist",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caritosteph/node.locomote.test.git"
  },
  "author": "Carolyn Ulfe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caritosteph/node.locomote.test/issues"
  },
  "homepage": "https://github.com/caritosteph/node.locomote.test#readme"
}
